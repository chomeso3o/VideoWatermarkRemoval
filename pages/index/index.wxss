.banner {
    background-color: #1a7244;
    height: 170rpx;
    position: relative;
    width: 100%;
}

.content {
    color: #fff;
    font-size: 30rpx;
    height: 300rpx;
    line-height: 70rpx;
    padding-left: 100rpx;
    position: absolute;
    width: 400rpx;
}

.content_item {
    align-items: center;
    display: flex;
}

.dian {
    background-color: #fff;
    border-radius: 50%;
    display: block;
    height: 15rpx;
    margin-right: 15rpx;
    width: 15rpx;
}

.banner_img {
    height: 310rpx;
    position: absolute;
    right: 100rpx;
    top: 20rpx;
    width: 150rpx;
}

.analysis {
    border: 5rpx solid #0e2d85;
    border-radius: 20rpx;
    height: 200rpx;
    margin-left: 10%;
    margin-top: 40rpx;
    width: 80%;
}

.btn {
    display: flex;
    justify-content: space-around;
    margin-top: 60rpx;
    padding-bottom: 50rpx;
    width: 100%;
}

.qushuiying {
    background-color: #07c160;
    color: #fff;
}

.clear,.qushuiying {
    border-radius: 10rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    width: 40%;
}

.clear {
    border: 1px solid #ccc;
    color: #000;
}

.clear,.list {
    background-color: #fff;
}

.list {
    border: 1rpx solid #ccc;
    border-radius: 10rpx;
    color: #363636;
    font-size: 30rpx;
    margin-left: 10%;
    width: 80%;
}

.list_item {
    border-bottom: 1rpx solid #ccc;
    height: 80rpx;
    line-height: 80rpx;
    position: relative;
    text-indent: 30rpx;
    width: 100%;
}

.other_item {
    border: none;
}

.arrowhead {
    color: silver;
    font-size: 40rpx;
    position: absolute;
    right: 30rpx;
    top: 0;
}

.feedback {
    background-color: #fff;
    color: #000;
    margin-top: 50rpx;
}